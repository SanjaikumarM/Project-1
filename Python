Choice=int(input("Enter the Semester Number:"))

if(Choice==1):
    print("------------------------->SEMESTER 1<-------------------------")
    AC=int(input("Enter the Applied Chemistry credit points gained:"))
    CL=int(input("Enter the Chemistry Laboratory credit points gained:"))
    PPS=int(input("Enter the Programming for Problem Solving credit points gained:"))
    PPSL=int(input("Enter the Programming for Problem Solving lab credit points gained:"))
    CS=int(input("Enter the Communication Skills credit points gained:"))
    CAD=int(input("Enter the Calculas And Differential Equation credit points gained:"))
    EM=int(input("Enter the Engineering Mechanics credit points gained:"))
    Total1=CS*2+CAD*4+AC*3+EM*4+PPS*3+CL*2+PPSL*2
    SGPA1=Total1/20
    print("SGPA of SEMESTER 1 is:",SGPA1)
    CGPA=SGPA1
    print("CGPA in SEMESTER 1 is:",CGPA)

elif(Choice==2):
    print("------------------------->SEMESTER 2<-------------------------")
    BEE=int(input("Enter the Basic Electrical Engineering credit points gained:"))
    CSII=int(input("Enter the Communucation Skills II credit points gained:"))
    LTCV=int(input("Enter the Laplace Transform and Complex Variables credit points gained:"))
    EG=int(input("Enter the Engineering Graphics credit points gained:"))
    EPL=int(input("Enter the Engineering Practices Laboratory credit points gained:"))
    SO=int(input("Enter the Semiconductor Optoelectronics credit points gained:"))
    APL=int(input("Enter the Applied Physics Laboratory credit points gained:"))
    SGPA1=float(input("Enter SGPA in Semester1"))
    Total2=CSII*2+LTCV*4+SO*3+BEE*3+EG*4+APL*2+EPL*2
    SGPA2=Total2/20
    print("SGPA of SEMESTER 2 is:",SGPA2)
    CGPA=(SGPA1+SGPA2)/2
    print("CGPA in SEMESTER 2 is:",CGPA)
    
elif(Choice==3):
    print("------------------------->SEMESTER 3<-------------------------")
    PS=int(input("Enter the Probability and Statistics credit points gained:"))
    DS=int(input("Enter the Data Structures credit points gained:"))
    OOP=int(input("Enter the Object Oriented Programming credit points gained:"))
    DLC=int(input("Enter the Digital Logic Circuits credit points gained:"))
    SE=int(input("Enter the Software Engineering credit points gained:"))
    DSL=int(input("Enter the Data Structures Laboratory credit points gained:"))
    OOPL=int(input("Enter the Object Oriented Programming Laboratory credit points gained:"))
    SGPA1=float(input("Enter SGPA in Semester1"))
    SGPA2=float(input("Enter SGPA in Semester2"))
    Total3=PS*4+DS*3+OOP*3+DLC*5+SE*4+DSL*2+OOPL*2
    SGPA3=Total3/23
    print("SGPA of SEMESTER 3 is:",SGPA3)
    CGPA=(SGPA1+SGPA2+SGPA3)/3
    print("CGPA in SEMESTER 3 is:",CGPA3)

elif(Choice==4):
